.uiContainer {
  margin: 10px 0px 5px 10px;
  position: fixed;
  bottom: 0;
}

.uiTitle {
  font-size: 28px;
  font-family: "RaleWay";
  font-style: "normal";
  font-weight: 300;
  color: white;
  bottom: 0;
}

.uiDesc {
  font-size: 20px;
  font-family: "RaleWay";
  font-style: "normal";
  font-weight: 200;
  color: white;
}

.weui-demo-dialog {
  visibility: hidden;
  opacity: 0;
  transition: opacity 0.3s;
}

.weui-demo-dialog_show {
  visibility: visible;
  opacity: 1;
}

.uiButton {
  color: white;
  font-size: 18px;
  font-family: "RaleWay";
  font-style: "normal";
  font-weight: 500;
  background-color: #ff2d56;
  width: 85%;
}

.hidden {
  opacity: 0;
}

::-webkit-scrollbar {
     display: none;
}